"# Only listen for connections from the local machine.\\n" +
"Listen 127.0.0.1:631\\n" +
"\\n" +
"\\n" +
"# Show shared printers on the local network.\\n" +
"Browsing On\\n" +
"BrowseOrder allow,deny\\n" +
"BrowseAllow @LOCAL\\n";
}
