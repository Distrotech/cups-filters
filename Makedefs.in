AR	=	@AR@
ARFLAGS	=	crvs
BINDIR	=	@bindir@
CC	=	@CC@
CFLAGS	=	$(OPTIM) -I.. $(OPTIONS)
CP	=	@CP@
CXX	=	@CXX@
CXXFLAGS =	$(OPTIM) -I.. $(OPTIONS)
DATADIR	=	@datadir@
FLUID	=	@FLUID@
HTMLDOC	=	@HTMLDOC@
IMGLIBS	=	@IMGLIBS@ $(LIBS) -lm
INCDIR	=	@includedir@/cups
LDFLAGS	=	@LDFLAGS@ $(OPTIM)
LIBDIR	=	@libdir@
LIBEXEC =	@libexec@
LIBS	=	@LIBS@ -L../cups -lcups
LN	=	/bin/ln -sf
LOCAL	=	@localstatedir@
MANDIR	=	@mandir@
MKDIR	=	@MKDIR@ -p
NROFF	=	@GROFF@ -T ascii
OPTIM	=	@OPTIM@
OPTIONS	=
RANLIB	=	@RANLIB@
RM	=	@RM@ -f
SBINDIR	=	@sbindir@
SHELL	=	/bin/sh

.SUFFIXES:	.a .c .cxx .fl .h .man .o .z
.c.o:
	$(CC) $(CFLAGS) -c $<
.cxx.o:
	$(CXX) $(CXXFLAGS) -c $<
.fl.cxx .fl.h:
	$(FLUID) -c $<
.man.z:
	$(NROFF) -man $< >t
	pack -f t
	mv t $@

