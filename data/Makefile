include ../Makedefs

#
# Data files...
#

FILES	=	8859-1 8859-14 8859-15 8859-2 8859-3 8859-4 8859-5 \
		8859-6 8859-7 8859-8 8859-9 HPGLprolog psglyphs testprint.ps


#
# Make everything...
#

all:


#
# Clean all config and object files...
#

clean:


#
# Install files...
#

install:
	-$(MKDIR) $(DATADIR)/data
	$(CP) $(FILES) $(DATADIR)/data


